{
  "name": "tapcanvas-monorepo",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.8.1",
  "scripts": {
    "tap": "node packages/cli/bin/tapcanvas.js",
    "dev:web": "pnpm --filter @tapcanvas/web dev",
    "dev:api": "pnpm --filter api start:dev",
    "build:app": "pnpm --filter webcut run build:app",
    "build": "pnpm --filter @tapcanvas/web build",
    "build:api": "pnpm --filter api build",
    "test": "pnpm -w -r run test",
    "deploy:web:cf": "pnpm --filter @tapcanvas/web build && npx wrangler deploy",
    "deploy:webcut":"wrangler --config apps/webcut-main/wrangler.toml deploy"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "wrangler": "^3.90.0",
    "zod": "^3.25.76"
  }
}
